	name = Mario Uher
	email = uher.mario@gmail.com
[github]
	user = haihappen
[apply]
	whitespace = nowarn
[mergetool]
	keepBackup = true
[core]
	excludesfile = /Users/mario/.gitignore
	quotepath = false
	whitespace = -blank-at-eol
	editor = vim
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[alias]
	unstage = reset HEAD
	ls = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s" --decorate
[push]
	default = current
